{var img=$(this);var xpos=img.data("x");var ypos=img.data("y");var w=Math.ceil(img.data("columns")*OBJECTW);var h=Math.ceil(img.data("rows")*OBJECTH);var pixelx=Math.floor((xpos*OBJECTW)*scale);var pixely=Math.floor((ypos*OBJECTH)*scale);var pixelw=Math.floor(w*scale);var pixelh=Math.floor(h*scale);img.css('top',pixely+'px');img.css('left',pixelx+'px');img.css('width',pixelw+'px');img.css('height',pixelh+'px');});$(".lookbook-margin-tweak").each(function()
