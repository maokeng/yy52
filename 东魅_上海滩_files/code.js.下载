function getCSRFToken()
{return $("#csrfToken").text();}
function setCSRFToken(value)
{$("#csrfToken").text(value);if(typeof Storage!=="undefined")
{try{localStorage.setItem('csrfToken',value);}catch(e){localStorageCache.csrfToken=value;}}}
var snippetQueryCount=0;function postNextSnippet()
{if(pendingSnippets.length<=0)
{closeSnippetIndicator();return;}
var nextSnippet=pendingSnippets.shift();var params=nextSnippet.params;var cb=nextSnippet.cb;showSnippetIndicator();$(".alert").hide();var wrapper=function(data,text,xhr)
{if((xhr.readyState===4)&&(xhr.status===0))
{data={error:'Unknown network error. Please try again.'};}
if(data&&data.error&&data.error=="Invalid submit token.")
{location.reload();}
var csrf=xhr.getResponseHeader('X-SHT-CSRF-TOKEN');if(csrf)
setCSRFToken(csrf);tryCount=1;postNextSnippet();cb(data,text);}
var ajaxURL='/snippet.js?__ts='+(new Date).getTime()+(snippetQueryCount++)+Math.floor((Math.random()*100)+1);var pathArray=window.location.pathname.split('/');var secondLevelLocation=pathArray[1];if(secondLevelLocation.match(/^[a-z]{2,3}-[a-z]{2,4}$/ig))
{ajaxURL='/'+secondLevelLocation+ajaxURL;}
var domain=location.protocol+'//'+location.hostname+(location.port?':'+location.port:'');ajaxURL=domain+ajaxURL;var token=getCSRFToken();var tryCount=1;return $.ajax(ajaxURL,{type:'POST',data:params,headers:(token!='')?{'X-SHT-CSRF-TOKEN':token}:{},success:wrapper,cache:false,error:function(request,error)
{tryCount++;if(tryCount<=3)
{$.ajax(this);return;}
wrapper({error:(request.responseText?request.responseText:'Unknown Error. Please try again.')},request.responseText,request);}});}
function snippet(name,params,cb)
{params['_cmd']=name;pendingSnippets.push({params:params,cb:cb});postNextSnippet();}
