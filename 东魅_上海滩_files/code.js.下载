function addMarkers(data,nextZoomLevel)
{clearMarkers();var keys=Object.keys(data);var x;for(x=0;x<keys.length;x++)
{var key=keys[x];var coord=data[key];if(typeof BMap!=='undefined')
{var image='/assets/components/theme/img/pin.png';var myIcon=new BMap.Icon(image,new BMap.Size(32,32));var pt=new BMap.Point((parseFloat(coord.longitude)+0.00668),(parseFloat(coord.latitude)+0.00575));var marker=new BMap.Marker(pt);marker.data=coord;marker.setTitle(key);marker.setIcon(myIcon);map.addOverlay(marker);marker.infoWindow=data[key].popup;marker.addEventListener("click",function()
{var tmp=new BMap.InfoWindow(this.infoWindow);tmp.setWidth(250);this.openInfoWindow(tmp);});map.addOverlay(marker);attachEvent(marker,nextZoomLevel);}
else
{marker=new Object();marker.data=coord;}
markers.push(marker);}}
function inListing(needle,haystack)
{var x;for(x=0;x<haystack.length;x++)
{if(needle===haystack[x].name)
return true;}
return false;}
function updateSidebar()
{var html='';var zoomLevel=getZoomLevel(map.getZoom());var headers=null;var entries=null;var regionNames=Object.keys(coordinateData.regions);var storeNames=Object.keys(coordinateData.stores);var cityMapping={};var x;for(x=0;x<regionNames.length;x++)
{var region=regionNames[x];if(cityMapping[region]==null)
cityMapping[region]=[];var listing=cityMapping[region];var y;for(y=0;y<storeNames.length;y++)
{var store=coordinateData.stores[storeNames[y]];if(store.region==region)
{if(!inListing(store.city,listing))
listing.push({name:store.city});}}}
html+='<div class="panel-group" id="storeAccordion" role="tablist" aria-multiselectable="true">';for(var y=0;y<regionNames.length;y++)
{html+='<div class="panel panel-default">';html+='<div class="panel-heading" role="tab" id="heading-'+y+'">';html+='<h4 class="panel-title">';html+='<a role="button" class="collapse collapsed" data-toggle="collapse" data-parent="#storeAccordion" href="#region-'+y+'" aria-expanded="true" aria-controls="#region-'+y+'">'+regionNames[y]+'<span class="nav icon-plus-minus">&nbsp;</span></a>';html+='</h4>';html+='</div>';html+='<ul id="region-'+y+'" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading-'+y+'">';for(var z=0;z<cityMapping[regionNames[y]].length;z++)
{html+='<li><a class="city-entry" href="#">'+cityMapping[regionNames[y]][z].name+'</a><ul>';for(var zx=0;zx<coordinateData.stores.length;zx++)
{if(coordinateData.stores[zx].city==cityMapping[regionNames[y]][z].name)
{var entry=coordinateData.stores[zx];html+='<li>';html+='<div class="store-listing" data-priority="'+entry.priority+'">';html+='<div class="entry-heading"><a class="store-entry" data-index="'+entry.id+'" href="#">'+entry.name+'</a></div>';html+='<p>'+entry.address+'<br />';html+='<a class="store-entry" data-index="'+entry.id+'" href="tel:'+entry.contact+'">'+stores_translation['Tel:']+" "+entry.contact+'</a></p>';html+='<p><strong>'+stores_translation['OpeningHours']+'</strong><br />'+entry.hours+'</p>';html+='<p style="" class="text-right"><a target="_blank" href="/locate-a-store.html?print='+entry.id+'"><span class="glyphicon glyphicon-print"></span>'+stores_translation['Print']+'</a></p>';html+='</div>';html+='</li>';}}
html+='</ul></li>';}
html+='</ul>';html+='</div>';}
html+='</div>';$('.sidebar-store').html(html);$('.reset-map-view').click(function(e)
{e.preventDefault();resetView();});$('.city-entry').click(function(e)
{e.preventDefault();var coord=coordinateData.cities[$(this).html()];if(typeof BMap!=='undefined')
{map.centerAndZoom(new BMap.Point(parseFloat(coord.longitude)+0.00668,parseFloat(coord.latitude)+0.00575),STORE_ZOOM_LEVEL.min);}
else
{city_entry=$(this).text();}});$('.store-entry').click(function(e)
{e.preventDefault();var id=parseInt($(this).attr('data-index'));var x;for(x=0;x<markers.length;x++)
{var marker=markers[x];if(marker.data.id==id)
zoomToMarker(marker);}});}
function zoomToMarker(marker)
{map.closeInfoWindow();var tmp=new BMap.InfoWindow(marker.infoWindow);tmp.setWidth(250);marker.openInfoWindow(tmp);window.setTimeout(function()
{if(baidu_popup_fixed==0)
{baidu_popup_fixed=1;}},200);var zoomLevel=getZoomLevel(map.getZoom());if(zoomLevel==STORE_ZOOM_LEVEL)
map.centerAndZoom(marker.getPosition(),STORE_ZOOM_LEVEL.max);else
map.centerAndZoom(marker.getPosition(),STORE_ZOOM_LEVEL.min);}
function attachEvent(marker,nextZoomLevel)
{marker.addEventListener("click",function()
{zoomToMarker(marker);});}
function getZoomLevel(index)
{var x;for(x=0;x<zoomLevels.length;x++)
{if((index>=zoomLevels[x].min)&&(index<=zoomLevels[x].max))
return zoomLevels[x];}
return null;}
function clearMarkers()
{map.closeInfoWindow();map.clearOverlays();markers=[];}
function updateMapLocations()
{var zoomLevel=getZoomLevel(map.getZoom());if(oldZoom==zoomLevel)
return;if(zoomLevel==REGION_ZOOM_LEVEL)
{addMarkers(coordinateData.regions,CITY_ZOOM_LEVEL.min);}
else if(zoomLevel==CITY_ZOOM_LEVEL)
{addMarkers(coordinateData.cities,STORE_ZOOM_LEVEL.min);}
else if(zoomLevel==STORE_ZOOM_LEVEL)
{addMarkers(coordinateData.stores,400);}
oldZoom=zoomLevel;}
function resetView()
{if(infoWindow)
{infoWindow.close();infoWindow=null;}
clearMarkers();if(typeof BMap!=='undefined')
{map.centerAndZoom(new BMap.Point(40,23.241),2);}
else
{map.centerAndZoom(0,2);}}
function isPointInRect(point,bounds)
{if(!(point instanceof BMap.Point)||!(bounds instanceof BMap.Bounds))
{return false;}
var sw=bounds.getSouthWest();var ne=bounds.getNorthEast();return(point.lng>=sw.lng&&point.lng<=ne.lng&&point.lat>=sw.lat&&point.lat<=ne.lat);}
var stores_translation={'Tel:':'Tel:','OpeningHours':'Opening Hours','Print':'Print','Viewallstores':'View all stores'};function initializeMap()
{snippet('getTranslation',stores_translation,function(data)
{stores_translation=data;});if(typeof BMap!=='undefined')
{map=new BMap.Map("map-canvas");map.centerAndZoom(new BMap.Point(40,23.241),2);map.enableScrollWheelZoom();map.addEventListener("zoomend",function()
{if(coordinateData!=null)
{updateMapLocations();updateSidebar();}});map.addEventListener("dragend",function()
{if(coordinateData!=null)
{updateMapLocations();updateSidebar();}});}
snippet('Stores.getMapMarkers',{},function(data)
{coordinateData=data;updateMapLocations();updateSidebar();});}
$(document).ready(function()
{if($("#map-canvas").length>0&&typeof BMap!=='undefined')
{initializeMap();$("#map-canvas").each(initializeMap);}
else
{$("#map-canvas").html('<img style="width: 100%; height: auto" src="/assets/images/baidu_https.png" />');$("#map-canvas").css('background-color','#292929');initializeMap();}
