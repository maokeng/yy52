function check_same(customer)
{if(typeof customer.Charged!=='undefined'&&typeof customer.Shipping==='undefined')customer.Shipping=customer.Charged;if(customer.Shipping.ShipAdd1!=customer.Billing.BillAdd1)
return false;if(customer.Shipping.ShipAdd2!=customer.Billing.BillAdd2)
return false;if(customer.Shipping.ShipCity!=customer.Billing.BillCity)
return false;if(customer.Shipping.ShipDistrict!=customer.Billing.BillDistrict)
return false;if(customer.Shipping.ShipState!=customer.Billing.BillState)
return false;if(customer.Shipping.ShipCountry!=customer.Billing.BillCountry)
return false;return true;}
