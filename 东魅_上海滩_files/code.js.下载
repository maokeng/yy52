function submitOrder(cb)
{var steps=[submitShipping,submitDelivery,submitPackaging,submitPayment];var nextCaller=function(index)
{var next=steps[index];next(function(res)
{if(res.error)
{if(console&&console.log)
console.log(res.error);return cb(res);}
if(index==(steps.length-1))
return cb(res);nextCaller(index+1);});};nextCaller(0);}
